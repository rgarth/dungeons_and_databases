{
  "git": {
    "deploymentEnabled": {
      "main": true,
      "preview": false,
      "development": false
    }
  },
  "buildCommand": "prisma generate && next build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "env": {
    "SKIP_ENV_VALIDATION": "1"
  }
} 